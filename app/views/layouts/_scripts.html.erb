<script type = "text/javascript" charset="UTF-8">
 $(function() {
   $("a").on("mouseover", function(event) {
       this.style.color = "#368BC1"; //"Glacial blue ice"
       this.style.fontSize = "105%";
    });
   $("a").on("mouseout", function(event) {
       this.style.color = "";
       this.style.fontSize = "100%";
    });
 });
</script>
