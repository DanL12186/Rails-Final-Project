<script type = "text/javascript" charset="UTF-8">
 $(function() {
   $(".link").on("mouseover", function(event) {
       this.style.color = "red";
       this.style.fontSize = "110%";
    });
   $(".link").on("mouseout", function(event) {
       this.style.color = "";
       this.style.fontSize = "100%";
    });
 });
</script>
